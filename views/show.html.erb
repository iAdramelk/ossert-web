                                <th class="gems-stats-table__header-cell gems-stats-table__header-cell_values-column">Total</th>
                            </tr>
                        </thead>
                        <tbody class="gems-stats-table__body">
                            <tr
                                class="gems-stats-table__row"
                                data-title="Users Creating Issues"
                                data-description="Total number of unique users who created at least one issue."
                                data-ranks="[{&quot;type&quot;:&quot;a&quot;,&quot;year&quot;:100,&quot;total&quot;:300},{&quot;type&quot;:&quot;b&quot;,&quot;year&quot;:80,&quot;total&quot;:240},{&quot;type&quot;:&quot;c&quot;,&quot;year&quot;:60,&quot;total&quot;:120},{&quot;type&quot;:&quot;d&quot;,&quot;year&quot;:40,&quot;total&quot;:100},{&quot;type&quot;:&quot;e&quot;,&quot;year&quot;:20,&quot;total&quot;:80}]"
